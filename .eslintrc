{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommend",
    "plugins": [
        "@typescript-eslint/recommended",
        "prettier/recommended"
    ],
    "overrides": [
    ],
    "rules": {
        "no-unused-private-class-members": "error",
        "no-unused-expressions":"error"
    }
}
