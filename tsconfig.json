{
  "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": true,
    "target": "ES5",                                  
    "module": "ES6",
    "moduleResolution": "node",                                
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true                                 
  }
}
